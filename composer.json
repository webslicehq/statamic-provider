{
    "name": "webslicehq/statamic-provider",
    "description": "Webslice service provider for Statamic CMS applications",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "illuminate/support": "^10.0|^11.0",
        "illuminate/config": "^10.0|^11.0",
        "illuminate/log": "^10.0|^11.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "Webslice\\StatamicProvider\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Webslice\\StatamicProvider\\WebsliceServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}